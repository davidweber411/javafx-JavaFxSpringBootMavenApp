<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.wedasoft.javafxspringbootmavenapp.views.DialogController">
    <children>
        <GridPane hgap="5.0" vgap="5.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                <ColumnConstraints hgrow="ALWAYS" minWidth="10.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="100.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="springBeanValue:"/>
                <Label fx:id="springBeanValueLabel" text="Label" GridPane.columnIndex="1"/>
                <Label text="jfxAppValue: " GridPane.rowIndex="1"/>
                <Label fx:id="jfxAppValueLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
